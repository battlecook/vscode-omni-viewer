{
  "name": "omni-viewer",
  "displayName": "omni-viewer",
  "publisher": "battlecook",
  "description": "A VSCode extension for viewing audio, image, video, csv, and jsonl files",
  "version": "0.3.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/battlecook/vscode-omni-viewer.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "media",
    "audio",
    "video",
    "image",
    "CSV",
    "JSONL",
    "JSON",
    "viewers",
    "viewer"
  ],
  "icon": "img/icon.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "omni-viewer.audioViewer",
        "displayName": "Audio Viewer",
        "selector": [
          {
            "filenamePattern": "*.mp3"
          },
          {
            "filenamePattern": "*.wav"
          },
          {
            "filenamePattern": "*.ogg"
          },
          {
            "filenamePattern": "*.flac"
          },
          {
            "filenamePattern": "*.aac"
          },
          {
            "filenamePattern": "*.m4a"
          }
        ],
        "priority": "default"
      },
      {
        "viewType": "omni-viewer.videoViewer",
        "displayName": "Video Viewer",
        "selector": [
          {
            "filenamePattern": "*.mp4"
          },
          {
            "filenamePattern": "*.avi"
          },
          {
            "filenamePattern": "*.mov"
          },
          {
            "filenamePattern": "*.wmv"
          },
          {
            "filenamePattern": "*.flv"
          },
          {
            "filenamePattern": "*.webm"
          },
          {
            "filenamePattern": "*.mkv"
          }
        ],
        "priority": "default"
      },
      {
        "viewType": "omni-viewer.imageViewer",
        "displayName": "Image Viewer",
        "selector": [
          {
            "filenamePattern": "*.jpg"
          },
          {
            "filenamePattern": "*.jpeg"
          },
          {
            "filenamePattern": "*.png"
          },
          {
            "filenamePattern": "*.gif"
          },
          {
            "filenamePattern": "*.bmp"
          },
          {
            "filenamePattern": "*.webp"
          },
          {
            "filenamePattern": "*.svg"
          }
        ],
        "priority": "default"
      },
      {
        "viewType": "omni-viewer.csvViewer",
        "displayName": "CSV Viewer",
        "selector": [
          {
            "filenamePattern": "*.csv"
          },
          {
            "filenamePattern": "*.tsv"
          }
        ],
        "priority": "option"
      },
      {
        "viewType": "omni-viewer.jsonlViewer",
        "displayName": "JSONL Viewer",
        "selector": [
          {
            "filenamePattern": "*.jsonl"
          },
          {
            "filenamePattern": "*.ndjson"
          },
          {
            "filenamePattern": "*.jsonlines"
          }
        ],
        "priority": "option"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "omni-viewer.openCsvViewer",
          "when": "resourceExtname == '.csv' || resourceExtname == '.tsv'",
          "group": "navigation"
        },
        {
          "command": "omni-viewer.openJsonlViewer",
          "when": "resourceExtname == '.jsonl' || resourceExtname == '.ndjson' || resourceExtname == '.jsonlines'",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "omni-viewer.openCsvViewer",
        "title": "Open with CSV Viewer",
        "category": "Omni Viewer"
      },
      {
        "command": "omni-viewer.openJsonlViewer",
        "title": "Open with JSONL Viewer",
        "category": "Omni Viewer"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "package": "vsce package",
    "publish": "vsce publish",
    "publish:ovsx": "ovsx publish"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@types/jest": "^29.5.14",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.39.0",
    "jest": "^29.7.0",
    "path-browserify": "^1.0.1",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.4",
    "typescript": "^5.0.4",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "music-metadata": "^11.8.3"
  }
}
